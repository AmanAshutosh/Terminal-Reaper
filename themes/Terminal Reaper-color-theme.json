      "settings": {
        "foreground": "#B6F500"
      }
    },
    {
      "scope": ["source.scss keyword.control"],
      "settings": {
        "foreground": "#FFFADC"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#888A00"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#A4DD00"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FFFADC"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#B6F500"
      }
